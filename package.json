{
	"name": "fibrie",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"vite:dev": "vite dev --host --port=5173",
		"vite:build": "vite build",
		"vite:preview": "vite preview",
		"vite:preview:production": "node",
		"cap:sync": "npx cap sync",
		"cap:android:open": "npx cap open android",
		"cap:android:run:dev": "npx cap run android --live-reload --port=5173",
		"cap:android:run": "npx cap run android",
		"cap:ios:open": "npx cap open ios",
		"svelte:sync": "svelte-kit sync",
		"svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
		"svelte:check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings --watch",
		"eslint:fix": "eslint . --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint/.eslintcache --no-warn-ignored --fix ",
		"eslint:check": "eslint . --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint/.eslintcache --no-warn-ignored",
		"prettier:fix": "prettier --cache --cache-strategy content --write .",
		"prettier:check": "prettier --cache --cache-strategy content --check .",
		"code:fix": "npm run prettier:fix && npm run eslint:fix && npm run svelte:check",
		"code:check": "npm run prettier:check && npm run eslint:check && npm run svelte:check",
		"prepare": "npm run svelte:sync"
	},
	"engines": {
		"npm": "10.9.4",
		"node": "22.21.1"
	},
	"supportedArchitectures": {
		"os": [
			"win32",
			"linux",
			"darwin",
			"freebsd",
			"sunos"
		],
		"cpu": [
			"x64",
			"arm64",
			"arm",
			"ia32",
			"ppc64",
			"s390x"
		],
		"libc": [
			"glibc",
			"musl"
		]
	},
	"devDependencies": {
		"@eslint/compat": "2.0.1",
		"@eslint/js": "9.39.2",
		"@inlang/paraglide-js": "2.9.1",
		"@lucide/svelte": "0.563.1",
		"@sveltejs/adapter-static": "3.0.10",
		"@sveltejs/kit": "2.50.1",
		"@sveltejs/vite-plugin-svelte": "6.2.4",
		"@tailwindcss/forms": "0.5.11",
		"@tailwindcss/typography": "0.5.19",
		"@tailwindcss/vite": "4.1.18",
		"@trivago/prettier-plugin-sort-imports": "6.0.2",
		"@types/node": "25.1.0",
		"daisyui": "5.5.14",
		"eslint": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.14.0",
		"globals": "17.2.0",
		"prettier": "3.8.1",
		"prettier-plugin-multiline-arrays": "4.1.3",
		"prettier-plugin-svelte": "3.4.1",
		"prettier-plugin-tailwindcss": "0.7.2",
		"svelte": "5.49.0",
		"svelte-check": "4.3.5",
		"tailwindcss": "4.1.18",
		"typescript": "5.9.3",
		"typescript-eslint": "8.54.0",
		"vite": "7.3.1"
	},
	"dependencies": {
		"@capacitor/android": "8.0.2",
		"@capacitor/app": "8.0.0",
		"@capacitor/cli": "8.0.2",
		"@capacitor/core": "8.0.2",
		"@capacitor/device": "8.0.0",
		"@capacitor/ios": "8.0.2",
		"@capacitor/preferences": "8.0.0",
		"appwrite": "21.5.0",
		"posthog-js": "1.336.4",
		"valibot": "1.2.0"
	}
}
